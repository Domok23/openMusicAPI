{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "object-curly-newline": "off",
    "newline-per-chained-call": "off"
  }
}
